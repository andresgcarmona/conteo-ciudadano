<h1>Listing boxes</h1>
<table>
  <tr>
  	<th>Casilla</th>
    <th>Estado</th>
    <th>Numero</th>
    <th>Activa</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>
<% @boxes.each do |box| %>
  <tr>
  	<td><%= box.number %></td>
    <td><%= box.state.name %></td>
    <td><%= box.number %></td>
    <td><%= box.active %></td>
    <td><%= link_to 'Show', [:admin, box] %></td>
    <td><%= link_to 'Edit', edit_admin_box_path(box) %></td>
    <td><%= link_to 'Destroy', [:admin, box], :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>
<%= paginate @boxes %>
<br />
<%= link_to 'New Box', new_admin_box_path %>